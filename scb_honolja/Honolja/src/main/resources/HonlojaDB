------------------------------------------------------------------------------------------board--

CREATE TABLE board
(
    b_no                  number(4)         NOT NULL, 
    b_title               varchar2(500)     NOT NULL, 
    b_content             varchar2(3000)    NOT NULL, 
    b_date                date              NOT NULL, 
    b_member              number(10)        NULL, 
    b_viewcnt             number(4)         NULL, 
    b_originalFileName    varchar2(100)     NULL, 
    b_uploadFileName      varchar2(100)     NULL, 
    b_fileSize            number(20)        NULL, 
    u_id                  VARCHAR2(20)      NOT NULL, 
    CONSTRAINT BOARD_PK PRIMARY KEY (b_no)
);



CREATE SEQUENCE board_SEQ
START WITH 1
INCREMENT BY 1;



ALTER TABLE board
    ADD CONSTRAINT FK_board_u_id_users_u_id FOREIGN KEY (u_id)
        REFERENCES users (u_id);


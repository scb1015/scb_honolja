------------------------------------------------------------------------------------------img_reply--

CREATE TABLE img_reply
(
    ir_no         NUMBER(4)       NOT NULL, 
    ir_content    VARCHAR2(500)    NOT NULL, 
    ir_date       DATE            NOT NULL, 
    i_no          NUMBER(4)       NOT NULL, 
    u_id          VARCHAR2(20)    NOT NULL, 
    CONSTRAINT IMG_REPLY_PK PRIMARY KEY (ir_no)
);


CREATE SEQUENCE img_reply_SEQ
START WITH 1
INCREMENT BY 1;


ALTER TABLE img_reply
    ADD CONSTRAINT FK_img_reply_i_no_img_board_i_ FOREIGN KEY (i_no)
        REFERENCES img_board (i_no) on delete cascade;     


ALTER TABLE img_reply
    ADD CONSTRAINT FK_img_reply_u_id_users_u_id FOREIGN KEY (u_id)
        REFERENCES users (u_id) on delete cascade;

insert into img_reply values(img_reply_seq.nextval, '정말 멋있네요. 방문하고 싶어요', sysdate, 1, 'hb');
insert into img_reply values(img_reply_seq.nextval, '어딘지 물어봐도 될까요?', sysdate, 1, 'test');
insert into img_reply values(img_reply_seq.nextval, '나도 가봤어요. 강추 합니다.', sysdate, 1, 'sky');
insert into img_reply values(img_reply_seq.nextval, '작년 즈음에 갔었는데, 무지 불친절 해요.', sysdate, 1, 'sky');
insert into img_reply values(img_reply_seq.nextval, '맞아요 가지마세요.', sysdate, 1, 'hb');





ALTER TABLE boardreply
    ADD CONSTRAINT FK_img_reply_i_no_img_board_i_ FOREIGN KEY (i_no)
        REFERENCES img_board (i_no) on delete cascade;     



------------------------------------------------------------------------------------------img_like--

CREATE TABLE img_like
(
    il_no      NUMBER(4)       NOT NULL, 
    il_like    VARCHAR2(20)    NOT NULL, 
    u_id       VARCHAR2(20)    NOT NULL, 
    i_no       NUMBER(4)       NOT NULL, 
    CONSTRAINT IMG_LIKE_PK PRIMARY KEY (il_no)
);

CREATE SEQUENCE img_like_SEQ
START WITH 1
INCREMENT BY 1;


ALTER TABLE img_like
    ADD CONSTRAINT FK_img_like_u_id_users_u_id FOREIGN KEY (u_id)
        REFERENCES users (u_id) on delete cascade;


ALTER TABLE img_like
    ADD CONSTRAINT FK_img_like_i_no_img_board_i_n FOREIGN KEY (i_no)
        REFERENCES img_board (i_no) on delete cascade;

------------------------------------------------------------------------------------------img_reply_of_reply--

CREATE TABLE img_reply_of_reply
(
    irr_no         NUMBER(4)       NOT NULL, 
    irr_content    VARCHAR2(500)    NOT NULL, 
    irr_date       DATE            NOT NULL, 
    ir_no          NUMBER(4)       NOT NULL, 
    u_id           VARCHAR2(20)    NOT NULL, 
    CONSTRAINT IMG_REPLY_OF_REPLY_PK PRIMARY KEY (irr_no)
);

CREATE SEQUENCE img_reply_of_reply_SEQ
START WITH 1
INCREMENT BY 1;

ALTER TABLE img_reply_of_reply
    ADD CONSTRAINT FK_img_reply_of_reply_ir_no_im FOREIGN KEY (ir_no)
        REFERENCES img_reply (ir_no) on delete cascade;


ALTER TABLE img_reply_of_reply
    ADD CONSTRAINT FK_img_reply_of_reply_u_id_use FOREIGN KEY (u_id)
        REFERENCES users (u_id) on delete cascade;


insert into img_reply_of_reply values(img_reply_of_reply_SEQ.nextval, '정말이에요??', sysdate,5, 'hb');
insert into img_reply_of_reply values(img_reply_of_reply_SEQ.nextval, '거짓말 아니죠?', sysdate,5, 'sky');
insert into img_reply_of_reply values(img_reply_of_reply_SEQ.nextval, '걱정마세요.', sysdate,5, 'hb');

------------------------------------------------------------------------------------------boardrreply--

CREATE TABLE boardrreply
(
    brr_no         number(4)        NOT NULL, 
    brr_content    varchar2(100)    NOT NULL, 
    brr_date       date             NOT NULL, 
    br_no          number(4)        NOT NULL, 
    u_id           VARCHAR2(20)     NOT NULL, 
    depth          NUMBER(2)        NOT NULL, 
    b_no           number(4)        NULL, 
    CONSTRAINT BOARDRREPLY_PK PRIMARY KEY (brr_no)
)
/

CREATE SEQUENCE boardrreply_SEQ
START WITH 1
INCREMENT BY 1;

ALTER TABLE boardrreply
    ADD CONSTRAINT FK_boardrreply_u_id_users_u_id FOREIGN KEY (u_id)
        REFERENCES users (u_id)


ALTER TABLE boardrreply
    ADD CONSTRAINT FK_boardrreply_br_no_boardrepl FOREIGN KEY (br_no)
        REFERENCES boardreply (br_no) on delete cascade

ALTER TABLE boardrreply
    ADD CONSTRAINT FK_boardrreply_b_no_boardreply FOREIGN KEY (b_no)
        REFERENCES boardreply (b_no) 









------------------------------------------------------------------------------------------roominfo--

CREATE TABLE roominfo
(
    r_no         NUMBER(4)        NOT NULL, 
    r_name       VARCHAR2(50)     NOT NULL, 
    r_content    VARCHAR2(70)     NOT NULL, 
    r_price      NUMBER(10)       NOT NULL, 
    r_min        number(2)        NOT NULL, 
    r_people     number(4)        NULL, 
    g_no         NUMBER(4)        NOT NULL, 
    img_url      varchar2(100)    NOT NULL, 
    img_url2     varchar2(100)    NOT NULL, 
    img_url3     varchar2(100)    NOT NULL, 
    CONSTRAINT ROOMINFO_PK PRIMARY KEY (r_no)
);
/

CREATE SEQUENCE roominfo_SEQ
START WITH 1
INCREMENT BY 1;



ALTER TABLE roominfo
    ADD CONSTRAINT FK_roominfo_g_no_guesthouse_g_ FOREIGN KEY (g_no)
        REFERENCES guesthouse (g_no);
 

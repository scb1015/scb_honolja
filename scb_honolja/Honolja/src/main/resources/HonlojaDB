------------------------------------------------------------------------------------------review--

CREATE TABLE review
(
    re_no         NUMBER(4)        NOT NULL, 
    re_title      VARCHAR2(20)     NULL, 
    re_content    VARCHAR2(500)    NOT NULL, 
    re_like       VARCHAR2(20)     NOT NULL, 
    re_score      NUMBER(4)        NOT NULL, 
    re_date       DATE             NOT NULL, 
    re_img        VARCHAR2(20)     NULL, 
    u_id          VARCHAR2(20)     NOT NULL, 
    g_no          NUMBER(4)        NOT NULL, 
    CONSTRAINT REVIEW_PK PRIMARY KEY (re_no)
);


CREATE SEQUENCE review_SEQ
START WITH 1
INCREMENT BY 1;

ALTER TABLE review
    ADD CONSTRAINT FK_review_u_id_users_u_id FOREIGN KEY (u_id)
        REFERENCES users (u_id) on delete cascade;
/

ALTER TABLE review
    ADD CONSTRAINT FK_review_g_no_guesthouse_g_no FOREIGN KEY (g_no)
        REFERENCES guesthouse (g_no) on delete cascade;


------------------------------------------------------------------------------------------message--

CREATE TABLE message
(
    m_no         number(4)        NOT NULL, 
    m_title      varchar2(100)    NOT NULL, 
    m_content    varchar2(500)    NOT NULL, 
    m_id         varchar2(20)     NOT NULL, 
    m_date       date             NOT NULL, 
    u_id         varchar2(20)     NOT NULL, 
    m_check      VARCHAR2(15)     NULL, 
    m_send      VARCHAR2(10)     NULL, 
    m_receive      VARCHAR2(10)     NULL, 
    CONSTRAINT MESSAGE_PK PRIMARY KEY (m_no)
);


CREATE SEQUENCE message_SEQ
START WITH 1
INCREMENT BY 1;


ALTER TABLE message
    ADD CONSTRAINT FK_message_u_id_users_u_id FOREIGN KEY (u_id)
        REFERENCES users (u_id);

------------------------------------------------------------------------------------------boardreply--

CREATE TABLE boardreply
(
    br_no         number(4)        NOT NULL, 
    br_content    varchar2(100)    NOT NULL, 
    br_date       date             NOT NULL, 
    b_no          number(4)        NOT NULL, 
    u_id          VARCHAR2(20)     NOT NULL, 
    CONSTRAINT BOARDREPLY_PK PRIMARY KEY (br_no)
);


CREATE SEQUENCE boardreply_SEQ
START WITH 1
INCREMENT BY 1;


ALTER TABLE boardreply
    ADD CONSTRAINT FK_boardreply_b_no_board_b_no FOREIGN KEY (b_no)
        REFERENCES board (b_no);


ALTER TABLE boardreply
    ADD CONSTRAINT FK_boardreply_u_id_users_u_id FOREIGN KEY (u_id)
        REFERENCES users (u_id);

------------------------------------------------------------------------------------------boardlike--

CREATE TABLE boardlike
(
    bl_no      number(4)       NOT NULL, 
    bl_date    date            NOT NULL, 
    b_no       number(4)       NOT NULL, 
    u_id       VARCHAR2(20)    NOT NULL, 
    CONSTRAINT BOARDLIKE_PK PRIMARY KEY (bl_no)
);


CREATE SEQUENCE boardlike_SEQ
START WITH 1
INCREMENT BY 1;


ALTER TABLE boardlike
    ADD CONSTRAINT FK_boardlike_b_no_board_b_no FOREIGN KEY (b_no)
        REFERENCES board (b_no);


ALTER TABLE boardlike
    ADD CONSTRAINT FK_boardlike_u_id_users_u_id FOREIGN KEY (u_id)
        REFERENCES users (u_id);

------------------------------------------------------------------------------------------answer--
CREATE TABLE answer
(
    a_no           NUMBER(4)        NOT NULL, 
    u_guestname    VARCHAR2(30)     NOT NULL, 
    a_content      VARCHAR2(100)    NOT NULL, 
    a_date         DATE             NULL, 
    re_no          NUMBER(4)        NOT NULL, 
    CONSTRAINT ANSWER_PK PRIMARY KEY (a_no)
);
/

CREATE SEQUENCE answer_SEQ
START WITH 1
INCREMENT BY 1;


ALTER TABLE answer
    ADD CONSTRAINT FK_answer_re_no_review_re_no FOREIGN KEY (re_no)
        REFERENCES review (re_no) on delete cascade;


------------------------------------------------------------------------------------------noticeboard--

CREATE TABLE noticeboard
(
    n_no         NUMBER(4)         NOT NULL, 
    n_title      VARCHAR2(500)     NOT NULL, 
    n_content    VARCHAR2(2000)    NOT NULL, 
    n_date       DATE              NOT NULL, 
    n_viewcnt    NUMBER(4)         NOT NULL, 
    u_id         VARCHAR2(20)      NOT NULL, 
    n_fix        VARCHAR2(20)      NOT NULL, 
    CONSTRAINT NOTICEBOARD_PK PRIMARY KEY (n_no)
);


CREATE SEQUENCE noticeboard_SEQ
START WITH 1
INCREMENT BY 1;



ALTER TABLE noticeboard
    ADD CONSTRAINT FK_noticeboard_u_id_users_u_id FOREIGN KEY (u_id)
        REFERENCES users (u_id);


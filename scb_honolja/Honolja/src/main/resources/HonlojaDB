------------------------------------------------------------------------------------------reservation--

CREATE TABLE reservation
(
    res_no        NUMBER(4)       NOT NULL, 
    u_id          VARCHAR2(20)    NOT NULL, 
    res_price     NUMBER(10)      NOT NULL, 
    res_ok        CHAR            NOT NULL, 
    res_people    number(4)       NOT NULL, 
    res_date      DATE            NOT NULL, 
    check_in      DATE            NOT NULL, 
    check_out     DATE            NOT NULL, 
    r_no          NUMBER(4)       NOT NULL, 
    CONSTRAINT RESERVATION_PK PRIMARY KEY (res_no)
);



CREATE SEQUENCE reservation_SEQ
START WITH 1
INCREMENT BY 1;


ALTER TABLE reservation
    ADD CONSTRAINT FK_reservation_r_no_roominfo_r FOREIGN KEY (r_no)
        REFERENCES roominfo (r_no) on delete cascade;


ALTER TABLE reservation
    ADD CONSTRAINT FK_reservation_u_id_users_u_id FOREIGN KEY (u_id)
        REFERENCES users (u_id) on delete cascade;

------------------------------------------------------------------------------------------guestlike--

CREATE TABLE guestlike
(
    gl_no      number(4)       NOT NULL, 
    gl_date    DATE            NOT NULL, 
    u_id       VARCHAR2(20)    NOT NULL, 
    g_no       NUMBER(4)       NOT NULL, 
    CONSTRAINT GUESTLIKE_PK PRIMARY KEY (gl_no)
);

CREATE SEQUENCE guestlike_SEQ
START WITH 1
INCREMENT BY 1;


ALTER TABLE guestlike
    ADD CONSTRAINT FK_guestlike_u_id_users_u_id FOREIGN KEY (u_id)
        REFERENCES users (u_id);


ALTER TABLE guestlike
    ADD CONSTRAINT FK_guestlike_g_no_guesthouse_g FOREIGN KEY (g_no)
        REFERENCES guesthouse (g_no);


------------------------------------------------------------------------------------------img_board--

CREATE TABLE img_board
(
    i_no                  NUMBER(4)       NOT NULL, 
    i_title               VARCHAR2(100)    NOT NULL, 
    i_content             VARCHAR2(500)    NOT NULL, 
    i_date                DATE            NOT NULL, 
    i_viewcnt             NUMBER(4)     NOT NULL, 
    i_starcnt             NUMBER(4)     NOT NULL,	
    i_originalFileName    VARCHAR2(100)    NULL, 
    i_uploadFileName      VARCHAR2(100)    NULL, 
    i_fileSize            VARCHAR2(50)    NULL, 
    i_youtube  VARCHAR2(50)  NULL, 
    u_id                  VARCHAR2(20)    NOT NULL,
    CONSTRAINT IMG_BOARD_PK PRIMARY KEY (i_no)
);


CREATE SEQUENCE img_board_SEQ
START WITH 1
INCREMENT BY 1;



ALTER TABLE img_board
    ADD CONSTRAINT FK_img_board_u_id_users_u_id FOREIGN KEY (u_id)
        REFERENCES users (u_id);



------------------------------------------------------------------------------------------guesthouse--

CREATE TABLE guesthouse
(
    g_no            NUMBER(4)        NOT NULL, 
    g_name          VARCHAR2(200)    NOT NULL, 
    g_addr          VARCHAR2(200)    NOT NULL, 
    g_position_n    VARCHAR2(20)     NOT NULL, 
    g_detail        VARCHAR2(20)     NULL, 
    g_url           VARCHAR2(100)    NULL, 
    g_position_e    VARCHAR2(20)     NOT NULL, 
    CONSTRAINT GUESTHOUSE_PK PRIMARY KEY (g_no)
);


CREATE SEQUENCE guesthouse_SEQ
START WITH 1
INCREMENT BY 1;


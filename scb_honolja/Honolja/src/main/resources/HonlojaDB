------------------------------------------------------------------------------------------users--

CREATE TABLE users
(
    u_no            number(4)       NOT NULL, 
    u_id            VARCHAR2(20)    NOT NULL, 
    u_pwd           number(20)      NULL, 
    u_name          varchar2(10)    NOT NULL, 
    u_birth         varchar2(20)    NULL, 
    u_gender        varchar2(30)    NOT NULL, 
    u_phn           varchar2(50)    , 
    u_img           VARCHAR2(100)    NULL, 
    u_email         VARCHAR2(20)    NOT NULL, 
    u_member        VARCHAR2(20)    NULL, 
    u_date          DATE            NULL, 
    u_update        DATE            NULL, 
    u_emailkey      VARCHAR2(50)    NULL, 
    u_emailcheck    VARCHAR2(20)    NULL, 
    u_guestname     VARCHAR2(30)    NULL,
    u_postcode      VARCHAR2(20)    NULL,  
    u_guestjuso     VARCHAR2(100)   NULL,  
    u_guestnum      VARCHAR2(20)    NULL, 
    u_imgpath	    VARCHAR2(100)   NULL,  
    CONSTRAINT USERS_PK PRIMARY KEY (u_id)
);


CREATE SEQUENCE users_SEQ
START WITH 1
INCREMENT BY 1;

